module alu_tb();
reg [31:0] A,B;
reg [2:0] ALUControl;
wire [31:0] Result;
wire N,C,Z,V;

alu uut (A,B,ALUControl,Result,N,C,Z,V);
initial begin
ALUControl = 3'b000;
A = 32'b00000001000000010000000100000001;
B = 32'b00000001000000010000000100000000;
#10
ALUControl = 3'b001;
A = 32'b00000001000000010000000100000001;
B = 32'b00000001000000010000000100000000;
#10
ALUControl = 3'b010;
A = 32'b00000001000000010000000100000001;
B = 32'b00000001000000010000000100000000;
#10
ALUControl = 3'b011;
A = 32'b00000001000000010000000100000001;
B = 32'b00000001000000010000000100000000;
ALUControl = 3'b100;
A = 32'b00000001000000010000000100000001;
B = 32'b00000001000000010000000100000000;
ALUControl = 3'b101;
A = 32'b00000001000000010000000100000001;
B = 32'b00000001000000010000000100000000;
ALUControl = 3'b110;
A = 32'b00000001000000010000000100000001;
B = 32'b00000001000000010000000100000000;
ALUControl = 3'b111;
A = 32'b00000001000000010000000100000001;
B = 32'b00000001000000010000000100000000;

$finish;
end
endmodule
